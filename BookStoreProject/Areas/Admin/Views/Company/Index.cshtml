<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary text-dark">Company List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Company" asp-action="Upsert" class="btn btn-primary text-dark">
                <i class="bi bi-plus-square"></i> &nbsp; Create Company
            </a>
        </div>
    </div>
</div>
<br /> <br />

<table id="tblData" class="table table-bordered table-striped text-dark" style="width:100%">
    <thead>
        <tr>
            <th>Name</th>
            <th>Adress</th>
            <th>City</th>
            <th>State</th>
            <th>Phone Number</th>
            <th></th>
        </tr>
    </thead>
    
</table>

@section Scripts{
    <script src="~/js/company.js"></script>
}
